[run]
source = src
omit =
    */main.py
    */config/config.py
    */modules/autorizador/*
    __pycache__
    */test_*
    */tests/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    pass

precision = 2
show_missing = True
skip_covered = False

[html]
directory = htmlcov

